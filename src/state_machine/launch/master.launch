<launch>

   <include file="$(find ros_pololu_servo)/launch/pololu_example.launch"/>

   <node pkg="ros_pololu_servo" type="pololu_trajectory_action_node.py" name="pololu_trajectory_action_node" output="screen"/>

   <node pkg="state_machine" type="master_state_machine.py" name="master_state_machine" output="screen"/>

   <include file="$(find camera)/launch/camera.launch"/>

   <node pkg="camera" type="depth_listener.py" name="depth_listener" output="screen"/>

</launch>
